
<% i = 0 %>
<% if !@tags.nil? %>
  <% while i < @tags.count do %>
    The Links ID for: <b>"<%= @tags[i]%>"</b> are: <br/>
    <% j = 0 %>
    <% while j < @linksID[i].count %>
      The URL are: <%= child_array(@linksID[i][j]).as_json %><br/>
      <% j += 1 %>
    <%end%>
    <% i += 1 %>
  <% end %>
<% else %>
  <%= "You have no tags for the search" %>
<% end %>